<?xml version="1.0" encoding="UTF-8"?>
<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" >
  
   <Dialog
      title="Update Checkset"
      afterClose="dialogAftercloseUpdCH"
      >

      <beginButton>
           <Button
            text="Save"
            icon="sap-icon://save"
            press= "onSaveUpdCH"/>
      </beginButton>
      <endButton>
          <Button
            text="Cancel"
            icon="sap-icon://cancel"
            press= "onCloseDialogUpdCH"/>
      </endButton>
      
     <!--  <Text text = "Name : "/>-->
<content>
<VBox>
<Label text="CheckSet"/>
<Input   id="updateCheckset"  valueLiveUpdate="true" width="60%"/>
<Label text="Running number"/>
<Input   id="updateRunNumber" valueLiveUpdate="true" width="60%"/>
</VBox>
</content>

   </Dialog>
</core:FragmentDefinition>