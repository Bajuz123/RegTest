<?xml version="1.0" encoding="UTF-8"?>
<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">

	<Dialog title="{i18n>DeleteCheckset}" afterClose="dialogAftercloseDelCH">

		<beginButton>
			<Button text="{i18n>Yes}" icon="sap-icon://accept" press="onSaveDelCH" />
		</beginButton>
		<endButton>
			<Button text="{i18n>Cancel}" icon="sap-icon://cancel" press="onCloseDialogDelCH" />
		</endButton>

		<content>
			<VBox>
				<Label text="{i18n>DeleteConfirm}" />
			</VBox>
		</content>

	</Dialog>
</core:FragmentDefinition>