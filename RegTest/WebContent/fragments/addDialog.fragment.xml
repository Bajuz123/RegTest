<?xml version="1.0" encoding="UTF-8"?>
<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" >
  
   <Dialog
      title="Placeholders">
      <beginButton>
         <Button
            text="Close"
            press= "onCloseDialog"/>
      </beginButton>
     <!--  <Text text = "Name : "/>-->
	 <List id="list"
			        headerText="Placeholders"
			  	    items="{ path  : '/DATA/GROUPS',
			  	             sorter: { path     : 'NAME',
	                        			descending: false }
	                   }" >
		    	<StandardListItem title="{NAME}"
									           />
	  </List>

   </Dialog>
</core:FragmentDefinition>